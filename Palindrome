//Check whether sum of digits for an input number is Palindrome or not?
//The number should be <= 1000
//e.g. if 56 is passed as input number, then sum of its digits id 11 which is a palindrome.

#include<iostream>
#include<vector>
#include<numeric>
using namespace std;

//for n <= 1000
std::vector<int> GetDigitsOfNumber(int n)
    {
    std::vector<int> v;
    if (n < 1000)
        {
        int OnethDigit = n % 10;

        if (OnethDigit != n) // check whether n has only 1 digit
            {
            int temp = n / 10;
            int TenthDigit = temp % 10;
            if (TenthDigit/10 != 0) //check whether n has only 2 digits
                {
                int HundredthDigit = (temp / 10);
                v.push_back(HundredthDigit);
                }
            else if (n % 100 == 0)//check whether n is multiple of 100
                {
                int HundredthDigit = (temp / 10);
                v.push_back(HundredthDigit);
                }
            else if (temp % 10 == 0 && temp /10 != 0)//check whether n is a type of number such as 202
                {
                int HundredthDigit = (temp / 10);
                v.push_back(HundredthDigit);
                }
            else
                {
                int HundredthDigit = (temp / 10);
                if (HundredthDigit != 0)
                    {
                    v.push_back(HundredthDigit);
                    }
                }

            v.push_back(TenthDigit);
            }
        v.push_back(OnethDigit);
        return v;
        }

    if (n == 1000)
        {
        v.push_back(1);
        v.push_back(0);
        v.push_back(0);
        v.push_back(0);        
        }    
    }
    
    std::string CheckForPalindromeNumber(std::vector<int> v)
    {
    int size = v.size();
   
    for (int i = 0; i < v.size(); ++i)
        {
        if (v[i] != v[size - 1 - i])
            {
            return "NO";
            }
        }
   
    return "YES";
    }
    
    int SumElementsOfVector(std::vector<int> v)
    {   
    return std::accumulate(v.begin(), v.end(), 0);
    }

int main()
 {
	//code
	int nTestCase = 0;
	cin >> nTestCase;
	for(int i = 0; i < nTestCase; ++i)
	{
	int n = 0;
	cin >> n;
	std::vector<int> v = GetDigitsOfNumber(n);
	int sum = SumElementsOfVector(v);
	std::vector<int> v1 = GetDigitsOfNumber(sum);
	std::string result = CheckForPalindromeNumber(v1);
	cout << result << endl;
	}
	
	return 0;
}
